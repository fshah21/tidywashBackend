{
  "name": "userservice",
  "author": "Feya Shah",
  "license": "UNLICENSED",
  "version": "1.0.0",
  "description": "This is the user management backend service for Pinky Promise, containing user information for Doctors, Patients, Admins, etc ",
  "scripts": {
    "lint": "eslint --ext .js,.ts .",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "serve": "firebase emulators:start --inspect-functions=5001",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "main": "dist/index.js",
  "devDependencies": {
    "@eslint/js": "^9.24.0",
    "@types/node": "^22.14.1",
    "@typescript-eslint/eslint-plugin": "^8.29.1",
    "@typescript-eslint/parser": "^8.29.1",
    "eslint": "^9.24.0",
    "typescript": "^5.8.3"
  },
  "private": true,
  "dependencies": {
    "firebase-admin": "^13.2.0",
    "firebase-functions": "^6.3.2"
  }
}
